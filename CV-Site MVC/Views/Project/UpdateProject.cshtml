@model Project

<div asp-validation-summary="All" class="text-danger"></div>

<form asp-controller="Project" asp-action="UpdateProject" method="post">
    <table>

        <tr>
            <td>
                @*<label>Projektets id:</label>*@
                <input type="hidden" asp-for="Id" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Projektets namn:</label>
                <input asp-for="ProjectName" type="text" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Beskrivning av projektet:</label>
                <input asp-for="Description" type="text" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Startdatum:</label>
                <input asp-for="StartDate" type="datetime-local" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Slutdatum:</label>
                <input asp-for="EndDate" type="datetime-local" />
            </td>
        </tr>
        <tr>
            <td>
                <input type="submit" value="Uppdatera" class="submitbutton" />
            </td>
        </tr>
    </table>
</form>
