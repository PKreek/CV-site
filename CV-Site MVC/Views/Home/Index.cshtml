

@model List<CV>

	<div>
		<h3>CV</h3>
		<table class="table-bordered">
	<thead>
		<tr>
			<td>Id</td>
			<td>Utbildning</td>
			@*<td>Description</td>
			<td>Start date</td>*@
		</tr>
	</thead>
	<tbody>
			@foreach (var cV in Model)
			{
				<tr>
					<td>@cV.ID</td>
					<td>@cV.Utbildning</td>
				@*	<td>@prod.Description</td>
					<td>@prod.StartDate</td>*@
				</tr>
			}
	</tbody>
</table>
	</div>




<form method="post" asp-controller="Account" asp-action="LogOut">
	<AuthorizeView>
		<Authorized>
			<h2> You are logged in as @User.Identity.Name</h2>
			<input type="submit" value ="Log out"/>
		</Authorized>
	</AuthorizeView>
</form>


